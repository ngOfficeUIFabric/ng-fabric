{
  "env": {
    "browser": true
  },
  "parserOptions": {
    "sourceType": "script"
  },
  "globals": {
    "angular": true,
    "jQuery": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-console": "off"
  }
}
