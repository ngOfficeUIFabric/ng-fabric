{
  "version": "0.1.0",
  "language": "en",
  "ignoreWordsList": [
    "https",
    "npm",
    "NPM",
    "bower",
    "nuget",
    "NuGet",
    "cdnjs",
    "CDNJS",
    "devDependency",
    "minified",
    "unminified",
    "angular",
    "Angular",
    "angularjs",
    "AngularJS",
    "IE9",
    "OfficeUiFabric",
    "ngOfficeUiFabric",
    "ngOfficeUIFabric",
    "ngofficeuifabric",
    "ng-OfficeUIFabric",
    "ng-officeuifabric",
    "package-ngofficeuifabric"
  ],
  "mistakeTypeToStatus": {
    "Passive voice": "Warning",
    "Spelling": "Error",
    "Complex Expression": "Warning",
    "Hidden Verbs": "Information",
    "Hyphen Required": "Error",
    "Redundant Expression": "Information",
    "Did you mean...": "Information",
    "Repeated Word": "Error",
    "Missing apostrophe": "Error",
    "Cliches": "Warning",
    "Missing Word": "Warning",
    "Make I uppercase": "Error"
  },
  "languageIDs": [
    "markdown",
    "latex",
    "plaintext"
  ],
  "ignoreRegExp": [
    "/`(kb.*?)`/g",
    "/(http\\\\S*)/gm",
    "/\\\\(.*.png\\\\)/g",
    "/\\\\(.*.gif\\\\)/g",
    "/\\\\(.*.md\\\\)/g",
    "/\\\\(.*.jpg\\\\)/g",
    "/^((`{3}\\\\s*)(\\\\w+)?(\\\\s*([\\\\w\\\\W]+?)\\\\n*)\\\\2)\\\\n*(?:[^\\\\S\\\\w\\\\s]|$)/gm"
  ]
}
